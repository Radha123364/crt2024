import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str1=s.next();
     char[] chars=str1.toLowerCase().toCharArray();
        Arrays.sort(chars);
        String sortedstr1=new String(chars);
        System.out.println(sortedstr1);
}
}
